{
  "name": "readmemaker",
  "version": "1.0.0",
  "description": "A nodeJS plataform developed to help your project's readme github",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p nodemon browsersync",
    "nodemon": "nodemon app/server.js",
    "browsersync": "browser-sync start --proxy http://localhost:5000 --files 'public, app/views'"
  },
  "nodemonConfig": {
    "ignore": [
      "data.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-felinto/readme_maker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucas-felinto/readme_maker/issues"
  },
  "homepage": "https://github.com/lucas-felinto/readme_maker#readme",
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "nunjucks": "^3.2.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "npm-run-all": "^4.1.5"
  }
}